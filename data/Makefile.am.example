## Process this file with automake to create Makefile.in.

# This is a sample MAINTAINERCLEANFILES declaration for
# # the top-level Makefile.am of autotools-using projects
# # to properly remove files generated by autogen.sh.
# #
# # Read definitions for the variables used to see which
# # ones your project would need.
# #
# # Edit to your need.
#
# for git
MAINTAINERCLEANFILES = \
 	$(GITIGNORE_MAINTAINERCLEANFILES_TOPLEVEL) \
	$(GITIGNORE_MAINTAINERCLEANFILES_MAKEFILE_IN) \
	$(GITIGNORE_MAINTAINERCLEANFILES_M4_GETTEXT) \
	$(GITIGNORE_MAINTAINERCLEANFILES_M4_LIBTOOL)

# for bzr
MAINTAINERCLEANFILES = \
	$(BZRIGNORE_MAINTAINERCLEANFILES_TOPLEVEL) \
	$(BZRIGNORE_MAINTAINERCLEANFILES_MAKEFILE_IN) \
	$(BZRIGNORE_MAINTAINERCLEANFILES_M4_GETTEXT) \
	$(BZRIGNORE_MAINTAINERCLEANFILES_M4_LIBTOOL) 

# for svn
MAINTAINERCLEANFILES = \
	$(SVNIGNORE_MAINTAINERCLEANFILES_TOPLEVEL) \
	$(SVNIGNORE_MAINTAINERCLEANFILES_MAKEFILE_IN) \
	$(SVNIGNORE_MAINTAINERCLEANFILES_M4_GETTEXT) \
	$(SVNIGNORE_MAINTAINERCLEANFILES_M4_LIBTOOL)

# for cvs
MAINTAINERCLEANFILES = \
	$(CVSIGNORE_MAINTAINERCLEANFILES_TOPLEVEL) \
	$(CVSIGNORE_MAINTAINERCLEANFILES_MAKEFILE_IN) \
	$(CVSIGNORE_MAINTAINERCLEANFILES_M4_GETTEXT) \
	$(CVSIGNORE_MAINTAINERCLEANFILES_M4_LIBTOOL)

# for hg
MAINTAINERCLEANFILES = \
	$(HGIGNORE_MAINTAINERCLEANFILES_TOPLEVEL) \
